    "# %% Importar librer√≠as\n",
    "import pandas as pd\n",
    "import glob\n",
    "import os\n",
    "\n",
    "# %% Definir funci√≥n para leer y fusionar todos los CSV\n",
    "def fusionar_csv_carpeta(ruta_carpeta):\n",
    "    \"\"\"\n",
    "    Lee todos los archivos CSV en la carpeta especificada y   \n",
    "    los fusiona en un solo DataFrame.\n",
    "    \n",
    "    Par√°metros:\n",
    "        ruta_carpeta (str): Ruta de la carpeta que contiene los archivos CSV.\n",
    "\n",
    "    Retorna:\n",
    "        pd.DataFrame: DataFrame con los datos combinados de todos los CSV.\n",
    "    \"\"\"\n",
    "    # Crear una lista con todos los archivos CSV en la carpeta\n",
    "    archivos_csv = glob.glob(os.path.join(ruta_carpeta, \"*.csv\"))\n",
    "    \n",
    "    # Verificar si se encontraron archivos\n",
    "    if not archivos_csv:\n",
    "        print(\"‚ö†Ô∏è No se encontraron archivos CSV en la ruta especificada.\")\n",
    "        return pd.DataFrame()\n",
    "    \n",
    "    # Leer y combinar todos los CSV\n",
    "    lista_df = []\n",
    "    for archivo in archivos_csv:\n",
    "        try:\n",
    "            df_temp = pd.read_csv(archivo, sep=';', encoding='utf-8')\n",
    "            lista_df.append(df_temp)\n",
    "        except Exception as e:\n",
    "            print(f\"Error leyendo {archivo}: {e}\")\n",
    "    \n",
    "    # Concatenar todos los DataFrames\n",
    "    df = pd.concat(lista_df, ignore_index=True)\n",
    "    return df\n"
   ]
